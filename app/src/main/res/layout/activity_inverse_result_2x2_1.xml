<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".calculate_result_2x2_1">

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="377dp"
        android:layout_height="176dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/result"
        tools:layout_editor_absoluteY="27dp" />

    <EditText
        android:id="@+id/et_1a"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:background="@drawable/edit_text_borders"
        android:editable="false"
        android:textAlignment="center"
        android:textColor="#EF4646"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/et_3a"
        app:layout_constraintEnd_toStartOf="@+id/et_2a"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView14" />

    <EditText
        android:id="@+id/et_2a"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:background="@drawable/edit_text_borders"
        android:editable="false"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="#EF4646"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/et_8a"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/et_1a"
        app:layout_constraintTop_toBottomOf="@+id/imageView14" />

    <EditText
        android:id="@+id/et_3a"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:background="@drawable/edit_text_borders"
        android:editable="false"
        android:textAlignment="center"
        android:textColor="#EF4646"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/et_8a"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/et_1a" />

    <EditText
        android:id="@+id/et_8a"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:background="@drawable/edit_text_borders"
        android:editable="false"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="#EF4646"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/et_3a"
        app:layout_constraintTop_toBottomOf="@+id/et_2a" />
</androidx.constraintlayout.widget.ConstraintLayout>